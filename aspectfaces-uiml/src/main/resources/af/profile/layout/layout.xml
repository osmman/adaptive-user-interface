<layout part-name="$field$Frame">
	<constraint>
		<alias name="left-of">$field$Label,$field$Entry</alias>
	</constraint>
	<constraint>
		<alias name="right-of">$field$Image,$field$Label</alias>
	</constraint>
	<constraint>
		<alias name="below">$field$Help,$field$Entry</alias>
	</constraint>
</layout>

